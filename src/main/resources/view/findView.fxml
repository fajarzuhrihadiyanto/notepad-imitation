<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" prefHeight="150.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controllers.FindController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="102.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <GridPane vgap="10.0" GridPane.columnIndex="1">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Button fx:id="findPrevButton" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Find Previous" />
            <Button fx:id="findNextButton" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Find Next" GridPane.rowIndex="1" />
            <Button fx:id="cancelButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Cancel" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <FlowPane hgap="10.0" prefWidth="200.0">
               <children>
                  <Label text="Find What :" />
                  <TextField fx:id="findField">
                     <FlowPane.margin>
                        <Insets />
                     </FlowPane.margin>
                  </TextField>
               </children>
            </FlowPane>
            <FlowPane hgap="10.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <CheckBox fx:id="isMatchCase" mnemonicParsing="false" text="Match Case" />
               </children></FlowPane>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
